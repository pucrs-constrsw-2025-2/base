# MongoDB
MONGODB_INTERNAL_HOST=mongodb
MONGODB_INTERNAL_PORT=27017
MONGODB_EXTERNAL_HOST=localhost
MONGODB_EXTERNAL_PORT=27017
MONGODB_USERNAME=admin
