# Altere para a URL base da sua instância do Keycloak
KEYCLOAK_SERVER_URL=http://localhost:8001

# Realm configurado no Keycloak
KEYCLOAK_REALM=your-realm

# Client ID configurado no Keycloak para esta API
KEYCLOAK_CLIENT_ID=your-client-id

# Client Secret (vá em Clients -> seu-client -> Credentials e copie o segredo)
KEYCLOAK_CLIENT_SECRET=your-client-secret

# Algoritmo de assinatura do token JWT (geralmente RS256 para Keycloak)
KEYCLOAK_TOKEN_ALGORITHM=your-token-algorithm
